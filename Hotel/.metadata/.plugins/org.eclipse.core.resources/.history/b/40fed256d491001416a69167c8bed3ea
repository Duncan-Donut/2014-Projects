
public class Hotel {
	 public static String name;
	 public int number;
	 public int room;
	 
	 public static void main(String[] args) {
		  Hotel Guest = new Hotel("Duncan",1,323);
		  Hotel.guestName();
		  System.out.println();
	  }

		  public Hotel(String A,int B, int C){
			  //constructor used to create the guests profile
			  name = A;
			  number = B;
			  room = C;
		  }
		  public  static String guestName(){
			  return name;
		  }
	  
}

