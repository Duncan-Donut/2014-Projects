
public class Hotel {
	
	  public static void main(String[] args) {
		  Hotel Guest = new Hotel("Duncan",1,323);
		  System.out.println(Guest);
	  }

		  public Hotel(String name,int number, int room){
			  //constructor used to create the guests profile
			  name = name;
			  number = number;
			  room = room;
		  }
	  
}
